{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sanjeevdf09"
		},
		"adssource_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adssource'"
		},
		"dbtest_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'dbtest'"
		},
		"adssource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagesa1.dfs.core.windows.net/"
		},
		"dbtest_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserversanjeev.database.windows.net"
		},
		"dbtest_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "test"
		},
		"dbtest_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sanjeev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sanjeevdf09_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adssource_accountKey": {
						"value": "[parameters('adssource_accountKey')]"
					},
					"dbtest_password": {
						"value": "[parameters('dbtest_password')]"
					},
					"adssource_properties_typeProperties_url": {
						"value": "[parameters('adssource_properties_typeProperties_url')]"
					},
					"dbtest_properties_typeProperties_server": {
						"value": "[parameters('dbtest_properties_typeProperties_server')]"
					},
					"dbtest_properties_typeProperties_database": {
						"value": "[parameters('dbtest_properties_typeProperties_database')]"
					},
					"dbtest_properties_typeProperties_userName": {
						"value": "[parameters('dbtest_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}